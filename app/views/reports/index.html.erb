<h1>Reports</h1>
<div class="row">
  <% @reports_list.each do |report| %>
  <div class="card m-2" style="width: 18rem;">
    <div class="card-body">
      <p>
        <h4 class="card-title"><%= report[:title] %></h4>  
      </p>
      <div class="d-flex flex-row justify-content-between align-items-center">
        <%= link_to "Run Report", report[:url], class: "btn btn-primary" %>
      </div>
    </div>
  </div>
  <% end %>
</div>